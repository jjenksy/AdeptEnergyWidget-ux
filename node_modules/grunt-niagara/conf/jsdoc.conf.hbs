{
  "tags"      : {
    "allowUnknownTags" : true
  },
  "plugins"   : ["plugins/markdown"],
  "templates" : {
    "cleverLinks"     : false,
    "monospaceLinks"  : false,
    "default"         : {
      "outputSourceFiles" : true
    },
    "systemName"      : "{{ name }}",
    "footer"          : "",
    "copyright"       : "{{ name }} Copyright © {{ copyrightYear }} Tridium. DocStrap Copyright © 2012-2013 the contributors to the JSDoc3 and DocStrap projects.",
    "navType"         : "vertical",
    "theme"           : "cerulean",
    "linenums"        : true,
    "collapseSymbols" : false,
    "inverseNav"      : true
  },
  "markdown"  : {
    "parser"   : "gfm",
    "hardwrap" : true
  }
}
